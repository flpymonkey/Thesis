    "import numpy as np\n",
    "\n",
    "# Relative entropy, also known as Kullback-Leibler (KL) divergence, \n",
    "# is used in the Exponential Gradient (EG) strategy to regularize the portfolio updates. \n",
    "# The idea is to keep the updated portfolio weights close to a reference distribution \n",
    "# (often the previous weights) to avoid drastic changes that could lead to overfitting or extreme volatility.\n",
    "\n",
    "def exponential_gradient_relative_entropy(x_t, eta=0.1, b_ref=None):\n",
    "    n, m = x_t.shape\n",
    "    if b_ref is None:\n",
    "        b_ref = np.ones(m) / m  # Reference uniform portfolio if not provided\n",
    "    b = np.ones(m) / m  # Initial uniform portfolio\n",
    "    portfolios = [b]\n",
    "    \n",
    "    for t in range(n):\n",
    "        # Update rule with relative entropy regularization\n",
    "        b = b * np.exp(eta * (x_t[t] - np.log(b / b_ref)))\n",
    "        b /= np.sum(b)  # Normalize to maintain sum to 1\n",
    "        portfolios.append(b)\n",
    "    \n",
    "    final_portfolio = portfolios[-1]\n",
    "    final_wealth = np.prod(np.dot(x_t, final_portfolio))\n",
    "    \n",
