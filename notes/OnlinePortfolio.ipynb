    "# Exponential growth rate for a strategy b_1^n\n",
    "\n",
    "def exponential_growth_rate(portfolios, price_relatives):\n",
    "    n = price_relatives.shape[0]\n",
    "    log_returns = []\n",
    "    \n",
    "    for t in range(1, n + 1):\n",
    "        wealth_increase = portfolio_wealth_increase(t, portfolios, price_relatives)\n",
    "        log_returns.append(np.log(wealth_increase))\n",
    "    \n",
    "    return np.mean(log_returns)\n",
    "\n",
    "# Calculate the exponential growth rate\n",
    "exp_growth_rate = exponential_growth_rate(portfolios, price_relatives)\n",
    "\n",
    "print(f\"Exponential Growth Rate: {exp_growth_rate}\")"
