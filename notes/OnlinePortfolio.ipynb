    "import numpy as np\n",
    "\n",
    "# Example closing prices for 3 assets over 4 periods\n",
    "closing_prices = np.array([\n",
    "    [100, 102, 105],\n",
    "    [101, 103, 106],\n",
    "    [99, 104, 107],\n",
    "    [102, 105, 108],\n",
    "])\n",
    "\n",
    "# Calculate the price relative vectors\n",
    "price_relatives = closing_prices / closing_prices[0, :]\n",
    "\n",
    "# Example portfolio vectors for 3 assets over 4 periods\n",
    "portfolios = np.array([\n",
    "    [0.4, 0.3, 0.3],  # Portfolio at t=1\n",
    "    [0.5, 0.2, 0.3],  # Portfolio at t=2\n",
    "    [0.3, 0.4, 0.3],  # Portfolio at t=3\n",
    "    [0.2, 0.5, 0.3],  # Portfolio at t=4\n",
    "])\n",
    "\n",
    "def portfolio_wealth_increase(t, portfolios, price_relatives):\n",
    "    b_t = portfolios[t-1]\n",
    "    x_t = price_relatives[t-1]\n",
    "    wealth_increase = np.dot(b_t, x_t)\n",
    "    return wealth_increase\n",
    "\n",
    "def cumulative_wealth(portfolios, price_relatives, initial_wealth):\n",
    "    cumulative_factor = 1\n",
    "    for t in range(1, price_relatives.shape[0] + 1):\n",
    "        \n",
    "        cumulative_factor *= portfolio_wealth_increase(t, portfolios, price_relatives)\n",
    "    return initial_wealth * cumulative_factor \n",
    "\n",
    "# Calculate the portfolio wealth increase for a specific period (e.g., t=2)\n",
    "t = 2\n",
    "wealth_increase_t = portfolio_wealth_increase(t, portfolios, price_relatives)\n",
    "\n",
    "print(f\"Portfolio wealth increase factor at t={t}: {wealth_increase_t}\")\n",
    "\n",
    "t = 4\n",
    "wealth_increase_t = portfolio_wealth_increase(t, portfolios, price_relatives)\n",
    "\n",
    "print(f\"Portfolio wealth increase factor at t={t}: {wealth_increase_t}\")\n",
    "\n",
    "\n",
    "\n",
    "# Initial wealth S_0\n",
    "initial_wealth = 1000\n",
    "\n",
    "# Calculate the cumulative wealth from period 1 to n\n",
    "final_wealth = cumulative_wealth(portfolios, price_relatives, initial_wealth)\n",
    "\n",
    "print(f\"Cumulative wealth: {final_wealth}\")\n"
