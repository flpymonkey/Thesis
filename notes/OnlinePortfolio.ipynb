    "import numpy as np\n",
    "\n",
    "# TODO fix this, not just selecting one stock, but shifting allocation between stocks\n",
    "\n",
    "def anti_correlation_follow_the_loser_stocks(x_t):\n",
    "    n, m = x_t.shape\n",
    "    cumulative_wealths = np.ones(m)\n",
    "    final_wealths = []\n",
    "    selected_stocks = []\n",
    "    \n",
    "    for t in range(n):\n",
    "        # Calculate the cumulative wealths using anti-correlation adjustment\n",
    "        performance = np.prod([x_t[i] for i in range(t+1)], axis=0)\n",
    "        cumulative_wealths = 1 / performance  # Inverse of performance for anti-correlation\n",
    "        \n",
    "        # Select the worst-performing stock (the loser)\n",
    "        loser = np.argmin(cumulative_wealths)\n",
    "        b_t = np.zeros(m)\n",
    "        b_t[loser] = 1.0  # Invest fully in the loser stock\n",
    "        \n",
    "        selected_stocks.append(loser)\n",
    "        final_wealth = np.dot(x_t[t], b_t)\n",
    "        final_wealths.append(final_wealth)\n",
    "    \n",
    "    cumulative_wealth = np.prod(final_wealths)\n",
    "    return cumulative_wealth, selected_stocks\n",
